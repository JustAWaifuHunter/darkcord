{
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.organizeImports": "never"
    },
    "files.associations": {
        "*.mdx": "markdown"
    },
    "files.insertFinalNewline": true,
    "files.eol": "\n",
    "files.exclude": {
        "**/node_modules": false,
        "**/tsconfig.json": false
    }
}

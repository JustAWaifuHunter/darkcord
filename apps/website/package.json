{
  "name": "@darkcord/website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --top-level generate:css && concurrently \"pnpm dev:css\" \"pnpm dev:next\"",
    "build:next": "next build",
		"build:css": "pnpm generate:css",
    "dev:next": "next dev",
    "dev:css": "pnpm generate:css --watch",
    "generate:css": "unocss src/**/*.tsx --out-file ./src/styles/unocss.css --config ../../unocss.config.ts",
    "lint": "pnpm eslint --ext .mjs,.js,.cjs,.ts,.tsx --fix **/src/**"
  },
  "type": "module",
  "contributors": [
		"Nicolas Ribeiro <business@niskii.dev>"
	],
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/JustAWaifuHunter/darkcord.git"
	},
	"bugs": {
		"url": "https://github.com/JustAWaifuHunter/darkcord/issues"
	},
  "dependencies": {
    "@vercel/og": "^0.1.0",
    "ariakit": "2.0.0-next.43",
    "cmdk": "^0.1.22",
    "next": "13.1.6",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-feather": "^2.0.10",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.1.6",
    "@types/node": "^18.14.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@unocss/cli": "^0.49.7",
    "@unocss/reset": "^0.49.7",
    "concurrently": "^7.6.0",
    "eslint": "^8.34.0",
    "eslint-config-next": "13.1.6",
    "typescript": "^4.9.5",
    "unocss": "^0.49.7"
  },
  "nextBundleAnalysis": {
    "budget": 358400,
    "budgetPercentIncreaseRed": 20,
    "showDetails": true
  },
  "engines": {
    "node": ">=16.9.0"
  }

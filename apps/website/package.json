{
  "name": "@darkcord/website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:next": "next start",
    "dev": "pnpm --stream --r docs && concurrently \"pnpm dev:css\" \"pnpm dev:next\"",
    "build:prod": "pnpm build:css && pnpm build:next",
    "build:next": "next build",
    "build:css": "pnpm generate:css",
    "dev:next": "next dev",
    "dev:css": "pnpm generate:css --watch",
    "generate:css": "unocss src/**/*.tsx --out-file ./src/styles/unocss.css --config ../../unocss.config.ts",
    "lint": "pnpm eslint --ext .mjs,.js,.cjs,.ts,.tsx --fix **/src/**"
  },
  "type": "module",
  "contributors": [
    "Nicolas Ribeiro <business@niskii.dev>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/JustAWaifuHunter/darkcord.git"
  },
  "bugs": {
    "url": "https://github.com/JustAWaifuHunter/darkcord/issues"
  },
  "dependencies": {
    "@microsoft/api-extractor-model": "^7.26.4",
    "@microsoft/tsdoc": "^0.14.2",
    "@next/bundle-analyzer": "^13.1.6",
    "@vercel/og": "^0.1.0",
    "ariakit": "2.0.0-next.43",
    "cmdk": "^0.1.22",
    "next": "13.1.6",
    "next-mdx-remote": "^4.3.0",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.7.1",
    "react-use": "^17.4.0",
    "shiki": "^0.14.0",
    "@unocss/cli": "^0.49.7",
    "@unocss/reset": "^0.49.7",
    "unocss": "^0.49.7",
    "rehype-ignore": "^1.0.4",
    "rehype-pretty-code": "^0.9.4",
    "rehype-raw": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1"
  },
  "devDependencies": {
    "@microsoft/tsdoc-config": "^0.16.2",
    "@types/node": "^18.14.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "concurrently": "^7.6.0",
    "eslint": "^8.34.0",
    "eslint-config-next": "13.1.6",
    "jju": "^1.4.0",
    "typescript": "^4.9.5"
  }
}

{
  "name": "darkcord",
  "version": "1.1.2",
  "description": "A NodeJS Package to interact with Discord API",
  "repository": {
    "type": "git",
    "url": "https://github.com/JustAWaifuHunter/darkcord.git",
    "directory": "packages/darkcord"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "typings": "./typings/index.d.ts",
  "scripts": {
    "c": "tsc && tscpaths -p tsconfig.json -s ./src -o ./typings && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "copy_wasm": "xcopy \"src\\utils\\zlib.wasm\" \"dist\\utils\"",
    "lint": "pnpm eslint --ext .ts --fix **/src/**",
    "build:docs": "tsc -p tsconfig.docs.json && tscpaths -p tsconfig.docs.json -s ./src -o ./typings-docs && pnpm downlevel-dts ./typings-docs ./typings-docs",
    "docs": "pnpm build:docs && api-extractor run --local"
  },
  "dependencies": {
    "@darkcord/interactions": "workspace:^",
    "@darkcord/rest": "workspace:^",
    "@darkcord/utils": "workspace:^",
    "@darkcord/ws": "workspace:^",
    "discord-api-types": "^0.37.33",
    "ts-mixer": "^6.0.3",
    "undici": "^5.19.1",
    "ws": "^8.12.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.34.4",
    "downlevel-dts": "^0.11.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.7",
    "erlpack": "^0.1.4",
    "tweetnacl": "^1.0.3",
    "zlib-sync": "^0.1.8"
  },
  "keywords": [
    "discord",
    "ws",
    "websocket",
    "gateway",
    "eris",
    "discord.js",
    "typescript",
    "ts",
    "javascript",
    "js",
    "http",
    "interactions",
    "zlib",
    "erlpack",
    "tweetnacl",
    "api",
    "wrapper",
    "undici"
  ]
}

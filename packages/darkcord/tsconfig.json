{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "baseUrl": "./src",
    "declarationDir": "typings",
    "paths": {
      "@utils/*": ["./utils/*"],
      "@client/*": ["./client/*"],
      "@resources/*": ["./resources/*"],
      "@typings/*": ["./types/*"],
      "@cache/*": ["./cache/*"],
      "@manager/*": ["./manager/*"]
    }
  },
  "exclude": ["dist"],
  "include": ["./src/**/*"]
}